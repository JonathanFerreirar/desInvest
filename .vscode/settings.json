{
  "prettier.configPath": "./.prettierrc.json",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "soucer.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.rulers": [80],
  "tailwindCSS.experimental.classRegex": [
    ["cn\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
}
